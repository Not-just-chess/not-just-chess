  <div class="col-10 center offset-1">
    <table id="chessboard">
      <tbody>
        <% [8, 7, 6, 5, 4, 3, 2, 1].each do |row| %>
          <tr>
            <% (1..8).each do |col| %>
              <td
              <% if (col + row).odd? %>
                class="white"
              <% else %>
                class="black"
              <% end %>
              >
              </td>
            <% end %>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>